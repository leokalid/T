const int ledPin = 12; // Define a porta do LED

void setup() {
  pinMode(ledPin, OUTPUT); // Define o pino do LED como sa√≠da
}

void loop() {
  digitalWrite(ledPin, HIGH); // Liga o LED
  delay(1000); // Espera por 1 segundo
  digitalWrite(ledPin, LOW); // Desliga o LED
  delay(1000); // Espera por 1 segundo
}
